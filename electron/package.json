{
  "name": "electron",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "electron index.js",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "Ryan Lee",
  "license": "ISC",
  "description": "Protect you eyes",
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "com.madeyes.app",
    "productName": "Mad Eyes",
    "files": [
      "**/**",
      "../web/**"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "extraResources": [
      {
        "from": "../web/.next",
        "to": "web/.next"
      },
      {
        "from": "../web/public",
        "to": "web/public"
      }
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "icon.ico"
    }
  }
}
